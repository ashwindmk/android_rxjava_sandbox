<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ashwin.rxjavasandbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RxJavaSandbox">
        <activity android:name=".subject.single.SingleSubjectActivity"></activity>
        <activity android:name=".subject.replay.ReplaySubjectActivity" />
        <activity android:name=".subject.behavior.BehaviorSubjectActivity" />
        <activity android:name=".subject.async.AsyncSubjectActivity" />
        <activity android:name=".subject.unicast.UnicastSubjectActivity" />
        <activity android:name=".subject.publish.PublishSubjectActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>